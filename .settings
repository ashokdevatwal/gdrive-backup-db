#########################################
# Backup Mode      		        #
#                                       #
# Local Backup => backupMode=local      #
# Gdrive Backup => backupMode=gdrive    #
#                                       #
#########################################

backupMode=gdrive

#################################
# Local Backup Path             #
#################################

localPath=/db_backup/

###################################
# Google Drive Backup Root Folder #
###################################

gdriveDir=DataBaseBackup

###############################
# Set false If Single DB      #
###############################

multiDB=true

##########################################
# Used For Single DB or Root Cerdentails #
##########################################

mysqlUser='root'
mysqlPassword=''

#################################################
# Multi DB with Multi user set become to false  #
#################################################

become=true

####################################################
# List of databases				   #
#						   #
# > Single DB => databaseName=dbname		   # 
# > Milti DB  => databaseName=(db1 db2 db3 db4)	   #
#						   #
####################################################
 
databaseName=(ars testdb)


